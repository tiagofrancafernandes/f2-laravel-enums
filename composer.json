{
    "name": "tiagof2/f2-laravel-enums",
    "description": "A Laravel package for enum handler",
    "license": "MIT",
    "keywords": [
        "classes",
        "enums",
        "helpers",
        "laravel",
        "tools",
        "tiagof2"
    ],
    "homepage": "https://tiagofranca.com",
    "authors": [
        {
            "name": "Tiago Fran√ßa",
            "email": "dev+composer-enums@tiagofranca.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "TiagoF2\\Enums\\": "src/"
        }
    },
    "require": {
        "php": "^7.2|^8.0|^8.1",
        "ext-json": "*",
        "illuminate/contracts": "^5.8|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/pagination": "^5.8|^6.0|^7.0|^8.0|^9.0|^10.0",
        "illuminate/support": "^5.8|^6.0|^7.0|^8.0|^9.0|^10.0",
        "tiagof2/f2-politool": "^2.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "TiagoF2\\Enums\\Providers\\LaravelEnumsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "preferred-install": "dist"
    }
}
